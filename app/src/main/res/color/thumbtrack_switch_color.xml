<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State Disabled -->
    <item android:color="@color/yp_gray" android:state_enabled="false" />

    <!-- State Checked -->
    <item android:color="@color/yp_dark_blue" android:state_checked="true" />

    <!-- State Unchecked -->
